{
  "name": "DDO Forum URL Redirect",
  "version": "0.3",
  "description": "Fix DDO Forums URLs.",
  "homepage_url": "https://github.com/SubniC/ddo-forum-redirector",
  "icons": { "16": "ddo16.png",
			 "48": "ddo48.png",
			 "128": "ddo128.png" },
  "background": { 
     "scripts":["background.js"]
     },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "http://*/*",
    "https://*/*"
  ]
}